SELECT * FROM Brexit_Tweets1;

DELETE FROM Brexit_Tweets1 WHERE round(ffRatio &gt; 2.8224,4);
SELECT count(*) FROM Brexit_Tweets1;

CREATE TABLE NormTweets AS
SELECT *, lower(Tweet) AS IcTweet
FROM Brexit_Tweets1;

DELETE FROM NormTweets WHERE (IcTweet like '%#stay%' OR IcTweet like '%#remain%') 
AND (IcTweet like '%#leave%' OR IcTweet like '%#brexit%');
SELECT count(*) FROM NormTweets;


SELECT count (*) AS remaincount FROM NormTweets WHERE (IcTweet like '%#stay%' OR IcTweet like '%#remain%');
SELECT count (*) AS leavecount FROM NormTweets WHERE (IcTweet like '%#leave%' OR IcTweet like '%#brexit%');




SELECT count (*) FROM NormTweets WHERE NOT(IcTweet LIKE '%stay%' OR IcTweet LIKE '%remain%' OR IcTweet like '%leave%');


SELECT count (*) 
FROM NormTweets 
WHERE (IcTweet like '%#stay%' OR IcTweet like '%#remain%') AND SentPos = '4';



SELECT  round (avg (Retweets),2) IcTweet, Retweets 
FROM NormTweets 
WHERE (IcTweet like '%#leave%') 
AND (SentPos + SentNeg) &gt; 0 
ORDER BY (Retweets) DESC; 

SELECT ScreenName,UserTweets,Tweet
FROM NormTweets 
GROUP By ScreenName
HAVING UserTweets &gt; 10000
ORDER BY (UserTweets) ASC;

